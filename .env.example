# Ultra-Advanced Portfolio Optimization System - Environment Variables
# ===================================================================
# Copy this file to .env and set your actual API keys and tokens

# Quantum Computing APIs
# =====================
# Get your IBM Quantum token from: https://quantum-computing.ibm.com/
IBM_QUANTUM_TOKEN=your_ibm_quantum_token_here

# Get your D-Wave token from: https://cloud.dwavesys.com/leap/
DWAVE_TOKEN=your_dwave_token_here

# Financial Data APIs
# ==================
# Get Alpha Vantage key from: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_KEY=your_alpha_vantage_key_here

# Get Quandl key from: https://data.nasdaq.com/
QUANDL_KEY=your_quandl_key_here

# Get News API key from: https://newsapi.org/
NEWS_API_KEY=your_news_api_key_here

# Database Configuration
# =====================
DB_USERNAME=portfolio_user
DB_PASSWORD=your_secure_password_here
REDIS_PASSWORD=your_redis_password_here
INFLUX_USERNAME=influx_user
INFLUX_PASSWORD=your_influx_password_here

# Security Configuration
# =====================
# Generate a secure JWT secret (32+ characters)
JWT_SECRET=your_very_secure_jwt_secret_key_with_32_plus_characters

# Generate a 32-character encryption key
ENCRYPTION_KEY=your_32_character_encryption_key_

# Optional: Trading Platform APIs
# ==============================
# Interactive Brokers
IB_API_HOST=127.0.0.1
IB_API_PORT=7497
IB_CLIENT_ID=1

# Alpaca Trading
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets  # Use paper trading initially

# System Configuration
# ===================
DEBUG_MODE=false
LOG_LEVEL=INFO
ENVIRONMENT=development

